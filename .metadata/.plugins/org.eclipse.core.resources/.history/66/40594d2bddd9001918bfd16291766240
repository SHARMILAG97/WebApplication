package com.revature.model;

import java.time.LocalDate;

public class Reserve {
	
	private LocalDate Date ;
	private int RId;
	private int RUId;
	private String RUName;
	private String RUAddress;
	private int Reservecans;
	private String Rstatus;
	
	
	public LocalDate getDate() {
		return Date;
	}
	public void setDate(LocalDate date) {
		Date = date;
	}
	public int getRId() {
		return RId;
	}
	public void setRId(int rId) {
		RId = rId;
	}
	public int getRUId() {
		return RUId;
	}
	public void setRUId(int rUId) {
		RUId = rUId;
	}
	public String getRUName() {
		return RUName;
	}
	public void setRUName(String rUName) {
		RUName = rUName;
	}
	public String getRUAddress() {
		return RUAddress;
	}
	public void setRUAddress(String rUAddress) {
		RUAddress = rUAddress;
	}
	public int getReservecans() {
		return Reservecans;
	}
	public void setReservecans(int reservecans) {
		Reservecans = reservecans;
	}
	public String getRstatus() {
		return Rstatus;
	}
	public void setRstatus(String rstatus) {
		Rstatus = rstatus;
	}
	

}
