<script type="text/javascript">

function View()
{
	var url = "http://localhost:8080/waterplant_web/ViewUsersServlet";
	$.getJSON(url, function(response){
		var data = response;
		document.getElementById("tbl").innerHTML = data;
		//for loop - table
	});
}


</script>
